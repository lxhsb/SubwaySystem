        <RadioButton x:Name="radioButton_Copy4" Content="6张" HorizontalAlignment="Left" Margin="10,286,0,0" VerticalAlignment="Top" Checked="radioButton_Copy7_Checked"/>
        <RadioButton x:Name="radioButton_Copy5" Content="7张" HorizontalAlignment="Left" Margin="10,322,0,0" VerticalAlignment="Top" Checked="radioButton_Copy7_Checked"/>
